{
	"name": "@azera/stack",
	"version": "1.1.34",
	"description": "Azera Web Framework",
	"main": "dist/index.js",
	"author": "Masoud Zohrabi <mdzzohrabi@gmail.com>",
	"license": "MIT",
	"bin": {
		"stack": "./bin/stack.js"
	},
	"files": [
		"dist/**/*",
		"*.MD",
		"tsconfig.json",
		"default.config.yml",
		"bin/**/*.js"
	],
	"scripts": {
		"bundle": "parcel src/index.ts --out-dir=./bundle --target=node --bundle-node-modules",
		"watch": "tsc -w",
		"build": "rimraf ./dist && tsc",
		"test": "sh ./scripts/test.sh",
		"test:watch": "sh ./scripts/test-watch.sh",
		"test-all": "mocha",
		"test-cover": "nyc mocha",
		"stack": "node bin/stack",
		"postinstall": "yarn stack init"
	},
	"dependencies": {
		"@azera/container": "^2.1.4",
		"@azera/reflect": "^1.0.9",
		"@azera/util": "^1.0.18",
		"@types/body-parser": "^1.19.2",
		"@types/deep-extend": "^0.6.0",
		"@types/express": "^4.17.17",
		"@types/express-fileupload": "^1.4.1",
		"@types/js-yaml": "^4.0.5",
		"@types/jsonwebtoken": "^9.0.2",
		"@types/microtime": "^2.1.0",
		"@types/node": "^20.2.5",
		"@types/twig": "^1.12.9",
		"body-parser": "^1.20.2",
		"commander": "^10.0.1",
		"deep-extend": "^0.6.0",
		"express": "^4.18.2",
		"express-fileupload": "^1.4.0",
		"express-validator": "^7.0.1",
		"graphql-tools": "^9.0.0",
		"http-proxy-agent": "^7.0.0",
		"https-proxy-agent": "^7.0.0",
		"js-yaml": "^4.0.0",
		"jsonwebtoken": "^9.0.0",
		"node-forge": "1.3.1",
		"reflect-metadata": "^0.1.13",
		"source-map-support": "^0.5.21",
		"spdy": "^4.0.2",
		"tslib": "^2.5.3",
		"twig": "^1.16.0",
		"winston": "^3.9.0"
	},
	"devDependencies": {
		"@graphql-tools/schema": "^10.0.0",
		"@graphql-tools/utils": "^10.0.0",
		"@types/amqplib": "^0.10.1",
		"@types/chai": "^4.3.5",
		"@types/mocha": "^10.0.1",
		"@types/mongoose": "^5.10.0",
		"@types/nodemailer": "^6.4.8",
		"@types/redis": "^4.0.11",
		"amqplib": "^0.10.3",
		"apollo-server": "^3.12.0",
		"apollo-server-express": "^3.12.0",
		"chai": "^4.3.7",
		"graphql": "^16.6.0",
		"jest": "^29.5.0",
		"mocha": "^10.2.0",
		"mongodb": "^5.6.0",
		"mongoose": "^7.2.2",
		"node-watch": "^0.7.3",
		"nodemailer": "^6.9.3",
		"nyc": "^15.1.0",
		"parcel-bundler": "^1.12.4",
		"redis": "^4.6.7",
		"rimraf": "^5.0.1",
		"ts-jest": "^29.1.0",
		"ts-node": "^10.9.1",
		"typeorm": "^0.3.16",
		"typescript": "^5.1.3"
	},
	"resolutions": {
		"node-forge": "0.10.0"
	},
	"nyc": {
		"extension": [
			".ts",
			".tsx"
		],
		"exclude": [
			"**/*.d.ts"
		],
		"reporter": [
			"html"
		],
		"all": true
	}
}
