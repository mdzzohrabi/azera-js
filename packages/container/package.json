{
    "name": "@azera/container",
    "description": "Javascript dependecy injection container",
    "authors": "Masoud Zohrabi <mdzzohrabi@gmail.com>",
    "version": "2.1.6",
    "main": "build/index.js",
    "types": "build/index.d.ts",
    "directories": {
        "lib": "build",
        "doc": "README.md",
        "test": "build/test"
    },
    "license": "MIT",
    "scripts": {
        "test": "cd src/tests & mocha -r ts-node/register *.spec.ts",
        "test:watch": "cd src/tests & mocha -r ts-node/register *.spec.ts --watch --watch-files ../*.ts,*.spec.ts",
        "build": "rimraf build & tsc -p tsconfig.json",
        "git:commit": "git add * & git commit -m",
        "git:push": "git push origin master",
        "clean": "rimraf build",
        "benchmark": "ts-node src/tests/benchmark.ts"
    },
    "files": [
        "build/**/*",
        "*.MD"
    ],
    "dependencies": {
        "@azera/reflect": "^1.0.9",
        "@azera/util": "^1.0.18",
        "glob": "^11.0.3",
        "reflect-metadata": "^0.2.2",
        "tslib": "^2.5.3"
    },
    "devDependencies": {
        "@types/glob": "^9.0.0",
        "@types/mocha": "^10.0.10",
        "expect": "^30.2.0",
        "mocha": "^11.7.4",
        "nyc": "^17.1.0",
        "rimraf": "^6.0.1",
        "ts-node": "^10.9.2",
        "typedi": "^0.10.0",
        "typescript": "^5.1.3"
    }
}
